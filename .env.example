# LogChain Environment Configuration
# Copy this file to .env and update with your actual values

# ChainMaker installation path (absolute path on host machine)
CHAINMAKER_PATH=/path/to/chainmaker-go

# ChainMaker node addresses (comma-separated if multiple nodes)
CHAINMAKER_NODE_HOST=your.chainmaker.host
CHAINMAKER_NODE_PORT_1=12301
CHAINMAKER_NODE_PORT_2=12302
CHAINMAKER_NODE_PORT_3=12303
CHAINMAKER_NODE_PORT_4=12304
